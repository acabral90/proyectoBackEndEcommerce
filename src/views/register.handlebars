<form action="/api/session/register" method="post" enctype="multipart/form-data" class="flex-col items-center inline-flex w-full px-6 py-12 lg:px-8" id="registerForm" >
 
 <div class="flex-col inline-flex items-center w-full border-b border-gray-900/10 pb-12">
      
      <h2 class="text-3xl m-10 font-bold leading-7 text-gray-900">Registro</h2> 
      
      
        <div class="">
          <label for="first-name" class=" text-sm font-medium leading-6 text-gray-900">Nombre</label>
          <div class="mt-2">  
            <input type="text" name="first_name" id="first-name" autocomplete="given-name" class="w-96 rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6">
          </div>
        </div>

        <div>
          <label for="last-name" class=" text-sm font-medium leading-6 text-gray-900">Apellido</label>
          <div class="mt-2">
            <input type="text" name="last_name" id="last-name" autocomplete="family-name" class="w-96 rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6">
          </div>
        </div>

        <div class="flex-col">
          <label for="email" class=" text-sm font-medium leading-6 text-gray-900">Email</label>
          <div class="mt-2">
            <input id="email" name="email" type="email" autocomplete="email" class="w-96 rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6">
            </div>
        </div>

        <div class="flex-col">
          <label for="street-address" class=" text-sm font-medium leading-6 text-gray-900">Edad</label>
          <div class="mt-2">
            <input type="text" name="age" autocomplete="street-address" class="w-96 rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6">
          </div>
        </div>

        <div class="flex-col">
          <label for="password" class=" text-sm font-medium leading-6 text-gray-900">Contraseña</label>
          <div class="mt-2">
            <input type="password" name="password" id="password" autocomplete="password" class="w-96 rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6">
          </div>
        </div>

        <div class="flex-col">
          <label for="profile" class=" text-sm font-medium leading-6 text-gray-900">Foto de perfil</label>
          <div class="mt-2">
            <input type="file" name="profile" id="profile" autocomplete="profile" class="w-96 rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6">
          </div>
        </div>
    </div>
    <button type="submit" class="w-52 rounded-md bg-indigo-600 px-3 py-2 text-sm font-semibold text-white shadow-sm  hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600">Guardar</button>
</form>

<p class="text-center mb-12">¿Ya tienes una cuenta? <a href="/" class="text-sm font-medium leading-6 text-gray-900">Ingrese aquí</a></p>

<script src="/js/register.js"></script>